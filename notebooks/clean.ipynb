   "source": [
    "import re\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Loads in player, league, and revenue mappings\n",
    "players = pd.read_csv('../data/data.csv', encoding='utf-8')\n",
    "league_team = pd.read_csv('../data/league_team_mapping.csv', encoding='utf-8')\n",
    "league_rev = pd.read_csv('../data/league_country_level_revenue_mapping.csv', encoding='utf-8')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merges league and revenue data together\n",
    "league = pd.merge(league_team, league_rev, on='League', how='left')\n",
    "league.columns = [f'League {col}' for col in league.columns]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merges players with merged league data\n",
    "players = pd.merge(players, league, left_on='Club', right_on='League Team', how='left')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Drops columns\n",
    "drop_cols = ['Unnamed: 0', 'ID', 'Name', 'Photo', 'Flag', 'Club Logo', \n",
    "             'Jersey Number', 'Joined', 'League Team', 'League League', \n",
    "             'Body Type', 'Wage', 'Release Clause']\n",
    "players = players.drop(drop_cols, axis=1, errors='ignore')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Gets rid of players with missing values\n",
    "players = players.loc[(~players['Preferred Foot'].isna())]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_money(df, colnames):\n",
    "    '''\n",
    "    Transforms string money data into numeric columns\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    for col in colnames:\n",
    "        df_copy[col] = df_copy[col].apply(parse_money)\n",
    "    return df_copy\n",
    "\n",
    "def parse_money(x):\n",
    "    ''' \n",
    "    Gets rid of any other strings besides numbers\n",
    "    from money variables\n",
    "    '''\n",
    "    if pd.isnull(x):\n",
    "        return x\n",
    "    m = re.search(r'€(\\d+\\.?\\d*)(\\w?)', x)\n",
    "    value = float(m.group(1))\n",
    "    unit = m.group(2)\n",
    "    if unit == 'K':\n",
    "        return value / 1000\n",
    "    if unit == 'M':\n",
    "        return value \n",
    "    return value / 1000000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "def del_no_value(df):\n",
    "    ''' Deletes players who have a value of NA '''\n",
    "    df_copy = df.copy()\n",
    "    return df.loc[df.Value > 0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_height(df, colnames):\n",
    "    '''\n",
    "    Transforms height columns to a numeric variable \n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    for col in colnames:\n",
    "        df_copy[col] = df_copy[col].apply(parse_height)\n",
    "    return df_copy\n",
    "\n",
    "def parse_height(x):\n",
    "    '''\n",
    "    Gets rid of any other strings besides numbers\n",
    "    from money variables\n",
    "    '''\n",
    "    if pd.isnull(x):\n",
    "        return x\n",
    "    m = re.search(r'(\\d+)\\'(\\d+)', x)\n",
    "    return int(m.group(1))*12 + int(m.group(2))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_weight(df, colnames):\n",
    "    '''\n",
    "    Transforms weight columns to a numeric variable\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    for col in colnames:\n",
    "        df_copy[col] = df_copy[col].apply(parse_weight)\n",
    "    return df_copy\n",
    "\n",
    "def parse_weight(x):\n",
    "    '''\n",
    "    To be put into transform_weight function. Gets rid of \n",
    "    unnecessary strings and convert it to numeric. \n",
    "    '''\n",
    "    if pd.isnull(x):\n",
    "        return x\n",
    "    m = re.search(r'(\\d+)lbs', x)\n",
    "    return int(m.group(1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_pos_ratings(df, colnames):\n",
    "    '''\n",
    "    Gets Player positional ratings and converts it \n",
    "    to a numeric column.\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    for col in colnames:\n",
    "        df_copy[col] = df_copy[col].apply(parse_pos_ratings)\n",
    "    df_copy['Is_Goalkeeper'] = df_copy.apply(lambda row: 0 if np.sum(row[colnames]) > 0 else 1, axis=1)\n",
    "    return df_copy\n",
    "\n",
    "def parse_pos_ratings(x):\n",
    "    '''\n",
    "    To be put into transform_pos_rating function. Gets rid of \n",
    "    unnecessary strings and converts it to numeric. \n",
    "    '''\n",
    "    if pd.isnull(x):\n",
    "        return 0\n",
    "    m = re.search(r'(\\d+)\\+\\d', x)\n",
    "    return int(m.group(1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_loaned(df):\n",
    "    '''\n",
    "    Sorts out rows of players that are loaned out\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    loaned_players_idx = ~df_copy['Loaned From'].isna()\n",
    "    df_copy.loc[loaned_players_idx, 'Club'] = df_copy.loc[loaned_players_idx, 'Loaned From']\n",
    "    df_copy.loc[loaned_players_idx, 'Contract Valid Until'] = df_copy.loc[loaned_players_idx, 'Contract Valid Until'].str.slice(-4)\n",
    "    df_copy['Loaned Out'] = 0\n",
    "    df_copy.loc[loaned_players_idx, 'Loaned Out'] = 1\n",
    "    df_copy = df_copy.drop('Loaned From', axis=1)\n",
    "    return df_copy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_work_rate(df):\n",
    "    '''\n",
    "    Transform Work Rate variables\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    df_copy['Work Rate'] = df_copy['Work Rate'].str.split('/')\n",
    "    df_copy['Off Work Rate'] = df_copy['Work Rate'].str[0]\n",
    "    df_copy['Def Work Rate'] = df_copy['Work Rate'].str[1]\n",
    "    df_copy = df_copy.drop('Work Rate', axis=1)\n",
    "    return df_copy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_league_revenue(df):\n",
    "    '''\n",
    "    Converts Revenue to Float\n",
    "    '''\n",
    "    df_copy = df.copy()\n",
    "    df_copy['League Revenue'] = df_copy['League Revenue'].str.replace(',', '').astype(float)\n",
    "    return df_copy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Performs all transformations using the variables above\n",
    "players = transform_money(players, ['Value'])\n",
    "players = transform_league_revenue(players)\n",
    "players = del_no_value(players)\n",
    "players = transform_weight(players, ['Weight'])\n",
    "players = transform_height(players, ['Height'])\n",
    "players = transform_pos_ratings(players, 'LS,ST,RS,LW,LF,CF,RF,RW,LAM,CAM,RAM,LM,LCM,CM,RCM,RM,LWB,LDM,CDM,RDM,RWB,LB,LCB,CB,RCB,RB'.split(','))\n",
    "players = transform_loaned(players)\n",
    "players = transform_work_rate(players)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Rename Columns names to have no space between strings\n",
    "players.columns = [col.replace(' ', '_') for col in players.columns]"
   ]
  },
